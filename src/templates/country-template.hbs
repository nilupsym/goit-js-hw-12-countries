{{#each this}}
<h2>{{name}}</h2>
<div><img src="{{flag}}" alt="{{flag.name}}" width="320" border="1"></div>
<div>
    <h3>Capital:</h3> {{capital}}
</div>
<div>
    <h3>Population:</h3> {{population}}
</div>
<div>
    <h3>Languages:</h3>
</div>
<ul>
    {{#each languages}}
    <li>
        {{name}}
    </li>
    {{/each}}
</ul>
{{/each}}